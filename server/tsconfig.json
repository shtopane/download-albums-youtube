{
    "compilerOptions": {
        "moduleResolution": "node",
        "strictPropertyInitialization": false,
        "sourceMap": true,
        "target": "es5",
        "types": [
            "node",
            "express"
        ],
        "module": "commonjs",
        "outDir": "./build",
        "typeRoots": [
            "./node_modules/ytdl-core/typings",
            "./node_modules/@types",
            "./typings"
        ],
        "lib": [
            "es2015"
        ]
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "node_modules",
    ]
}